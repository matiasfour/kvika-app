    "eslint-config-airbnb": ">=19.0.4",
    "eslint-config-prettier": ">=8.5.0",
    "eslint-import-resolver-typescript": ">=3.5.3",
    "eslint-plugin-import": ">=2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-jsx-a11y": ">=6.7.1",
    "eslint-plugin-react": ">=7.32.1",
    "eslint-plugin-react-hooks": ">=4.6.0",
    "eslint-plugin-react-native": "^4.0.0",
    "eslint-plugin-testing-library": ">=5.9.1",
    "expo-community-flipper": "^47.0.2",
    "husky": "^8.0.1",
    "jest": "^26.6.3",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "react-native-flipper": "^0.176.1",
    "react-native-svg-transformer": "^1.0.0",
    "styled-components": "^5.3.5",
    "stylelint": "^14.16.1",
    "stylelint-config-recommended": "^9.0.0",
    "stylelint-config-standard-scss": "^6.1.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "typescript": "^4.6.3"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,js,jsx,css}": [
      "prettier --write",
      "yarn lint",
      "yarn test --passWithNoTests"
    ]
  },
  "resolutions": {
    "react-devtools-core": "~4.25.0",
    "react-native-reanimated": "~2.12.0",
    "expo-linear-gradient": "~12.0.1"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg|d3|d3-array|internmap|delaunator|robust-predicates)"
