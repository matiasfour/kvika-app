{
  "name": "kvika-app",
  "version": "1.0.0",
  "description": "Kvika mobile app",
  "repository": "git@github.com:kvika/kvika-app.git",
  "main": "index.js",
  "scripts": {
    "start": "APP_VARIANT=development npx expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest",
    "lint": "yarn lint:ts && yarn lint:styles",
    "lint:ts": "eslint --ext .js,.tsx,.ts,.jsx src --max-warnings=0",
    "lint:styles": "stylelint './src/**/*.tsx'",
    "lint:staged": "lint-staged",
    "prepare": "husky install",
    "prettier:update": "prettier --write \"./src/**/*.{ts,tsx, js, css, scss, md}\" ",
    "prettier:base": "prettier --config ./.prettierrc 'src/**/*.{js,tsx,ts}'",
    "prettier:check": "yarn prettier:base --list-different",
    "postinstall": "patch-package"
  },
  "dependencies": {
